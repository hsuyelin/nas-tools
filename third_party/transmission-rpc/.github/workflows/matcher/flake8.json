{
  "problemMatcher": [
    {
      "owner": "flake8-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+): ([^W]*\\d\\d\\d) (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "flake8-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(\\d+): ([W]\\d\\d\\d) (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
